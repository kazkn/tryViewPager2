<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.tryviewpager2">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".GroupiePagerActivity1"/>
        <activity android:name=".GroupiePagerActivity2"/>
        <activity android:name=".GroupiePagerActivity3"/>
        <activity android:name=".FragmentPagerActivity1"/>
        <activity android:name=".ViewPagerActivity1"/>
        <activity android:name=".GroupiePagerActivity4"/>
    </application>

</manifest>